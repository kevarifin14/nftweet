{
  "name": "buidling-blocks",
  "packageManager": "yarn@1.22.17",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf .next && rm -rf yarn.lock",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "generate": "graphql-codegen --config codegen.yml",
    "db-console": "hasura console --no-browser --project hasura/",
    "db-up": "docker compose up -d",
    "db-deploy": "hasura migrate apply --database-name=default --project hasura/; hasura metadata apply --project hasura/",
    "db-down": "docker compose down --volumes",
    "dev": "next dev",
    "dx": "run-s db-up db-deploy dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "esbuild": "^0.14.30",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "next": "^12.0.8",
    "next-auth": "^4.3.4",
    "next-connect": "^0.12.2",
    "next-seo": "^5.4.0",
    "next-themes": "^0.1.1",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.31.1",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@tailwindcss/typography": "^0.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.32",
    "@types/react": "17.0.38",
    "@types/tailwindcss": "^3.0.10",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.16",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
